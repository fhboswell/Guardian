
<tr id="<%= individual.name %><%= @group.id %>">
	<td><%= individual.name %></td>
	<td><%= individual.check %></td>

	<% if current_user.type_key != "Guardian" %>
	
	<td><%= link_to "Deletee", group_individual_path(@group, individual.id), method: :delete, data: { confirm: " Are you sure?"} %></td>

	<td><%= link_to "Change Status", change_group_individual_path(@group, individual.id), method: :patch%></td>
	<% end %>

</tr>


